{
  "expo": {
    "name": "医智通",
    "slug": "yi-wen-tong",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "yiwentong",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/animeSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lumberjack5634.yiwentong"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3338c584-6c10-4f56-935d-09fed343d06a"
      }
    }
  }
}
